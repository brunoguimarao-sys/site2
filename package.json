{
	"name": "web-app",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "webpack serve --mode development --open",
		"build": "webpack --mode production",
		"dev": "webpack serve --mode development",
		"start:dev": "npx concurrently \"npm run dev\" \"npm start --prefix server\"",
		"start:prod": "npm run build && npx concurrently \"npm start --prefix server\" \"serve -s dist -l 4000\"",
		"setup": "npm install && npm install --prefix server"
	},
